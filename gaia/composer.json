{
    "name": "gaia/core",
    "version": "0.5.5",
    "license": "proprietary",
    "description": "Core PHP library running in PHP8.2",
    "require": {
        "php": "^8.2.26",
        "erusev/parsedown": "^1.7",
        "ratchet/pawl": "^0.4.1",
        "solarium/solarium": "^6.3",
        "reprovinci/solr-php-client": "^1.0",
        "pug/pug": "*",
        "pug-php/pug": "^3.5",
        "symfony/flex": "*",
        "twig/twig": "^2.16",
        "symfony/yaml": "^5.4",
        "johnpbloch/wordpress": "^6.7",
        "guzzlehttp/guzzle": "^7.9",
        "abraham/twitteroauth": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "core/",
            "Vivalibro\\": "vivalibro/",
            "Apiv1\\": "apiv1/",
            "Cubo\\": "cubos/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Core\\Tests\\": "tests/",
            "Vivalibro\\Tests\\": "tests/",
            "Apiv1\\Tests\\": "tests/",
            "Cubo\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "nodejs-php-fallback/nodejs-php-fallback": true,
            "johnpbloch/wordpress-core-installer": true
        }
    },
    "scripts": {
        "auto-scripts": []
    }
}
